.item-wrapper {
  flex: 1;
  width: 100%;
  padding: 50rpx 0;
}

.item-btn {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100%;
  font-size: 50rpx;
  background: lightskyblue;
  border: 1px solid #efefef;
  border-radius: 20rpx;
}

.hidden-portal {
  margin-right: 15rpx;
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  background: #000;
}